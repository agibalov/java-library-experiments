<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE jasperReport PUBLIC "-//JasperReports//DTD Report Design//EN"
        "http://jasperreports.sourceforge.net/dtds/jasperreport.dtd">
<jasperReport name="HelloReport">
    <parameter name="What" class="java.lang.String" />

    <field name="name" class="java.lang.String" />
    <field name="quantity" class="java.lang.Integer" />

    <title>
        <band height="50">
            <staticText>
                <reportElement x="0" y="0" width="100" height="50" />
                <text>Hello</text>
            </staticText>
            <textField>
                <reportElement x="100" y="0" width="100" height="50" />
                <textFieldExpression class="java.lang.String">$P{What}</textFieldExpression>
            </textField>
        </band>
    </title>
    <pageHeader>
        <band height="50">
            <staticText>
                <reportElement x="0" y="0" width="100" height="50" />
                <text>I am page header</text>
            </staticText>
        </band>
    </pageHeader>
    <columnHeader>
        <band height="50">
            <staticText>
                <reportElement x="0" y="0" width="100" height="50" />
                <text>Column Header</text>
            </staticText>
            <staticText>
                <reportElement x="100" y="0" width="100" height="50" />
                <text>Name</text>
            </staticText>
            <staticText>
                <reportElement x="200" y="0" width="100" height="50" />
                <text>Quantity</text>
            </staticText>
        </band>
    </columnHeader>
    <detail>
        <band height="50">
            <staticText>
                <reportElement x="0" y="0" width="100" height="50" />
                <text>I am detail</text>
            </staticText>
            <textField>
                <reportElement x="100" y="0" width="100" height="50" />
                <textElement />
                <textFieldExpression><![CDATA[$F{name}]]></textFieldExpression>
            </textField>
            <textField>
                <reportElement x="200" y="0" width="100" height="50" />
                <textElement />
                <textFieldExpression><![CDATA[$F{quantity}]]></textFieldExpression>
            </textField>
        </band>
    </detail>
    <pageFooter>
        <band height="50">
            <staticText>
                <reportElement x="0" y="0" width="100" height="50" />
                <text>I am page footer</text>
            </staticText>
        </band>
    </pageFooter>
    <summary>
        <band height="50">
            <staticText>
                <reportElement x="0" y="0" width="100" height="50" />
                <text>I am summary</text>
            </staticText>
        </band>
    </summary>
</jasperReport>
